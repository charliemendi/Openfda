import http.client
import json

headers = {'User-Agent': 'http-client'}

conn = http.client.HTTPSConnection("api.fda.gov")
conn.request("GET", "/drug/label.json?search=active_ingredient:acetylsalicylic&limit=100", None, headers)
r1 = conn.getresponse()
print(r1.status, r1.reason)
repos_raw = r1.read().decode("utf-8")
conn.close()

repos = json.loads(repos_raw)
limit=len(repos["meta"]["results"]["total"])#
Lista=[]
number=0
try :
    while number!=4:
        for i in range(0,limit):
            Lista.append(repos["results"][number]["openfda"]["manufacturer_name"])
        number+=1
except KeyError:
        Lista.append("Anonimo")
print("Los fabricantes son de estas aspirinas:")
for t in set(Lista):
    print(t)